                                            {
                                                <li>
                                                    <a href="@Url.Action("Roles")"><i class="fa fa-list-alt pull-left"></i>Editar roles</a>
                                                    @{ rol = true;}
                                                </li>
                                            }

                                            if ((permiso.PermisosId == (int)PermisosEnum.CrearUsuario) && usuario != true)
                                            {
                                                <li>
                                                    <a href="@Url.Action("Register")"><i class="fa fa-user pull-left"></i>Registrar Usuario</a>
                                                    @{ usuario = true;}
                                                </li>
                                            }
                                        }

